<Window x:Class="Assignment_85_Rawipote_P.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment_85_Rawipote_P"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TextBoxKey" HorizontalAlignment="Left" Height="82" Margin="30,28,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="743" GotFocus="TextBoxKey_GotFocus"/>
        <TextBox x:Name="TextBoxValue" HorizontalAlignment="Left" Height="86" Margin="30,131,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="743" GotFocus="TextBoxValue_GotFocus"/>
        <Button x:Name="AddBtn" Content="Add" HorizontalAlignment="Left" Height="141" Margin="30,263,0,0" VerticalAlignment="Top" Width="214" Click="AddBtn_Click"/>
        <Button x:Name="DeleteBtn" Content="Delete" HorizontalAlignment="Left" Height="141" Margin="288,263,0,0" VerticalAlignment="Top" Width="227" Click="DeleteBtn_Click"/>
        <Button x:Name="ShowBtn" Content="Show" HorizontalAlignment="Left" Height="141" Margin="561,263,0,0" VerticalAlignment="Top" Width="212" Click="ShowBtn_Click"/>

    </Grid>
</Window>
